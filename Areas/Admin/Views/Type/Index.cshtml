@model List<OnlineJewelleryShop.TypeMaster>
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<a href="@Url.Action("Detail","Type") " class="btn btn-primary">Add New</a>
<div style="text-align: Center">
    <h3><label>Product Type</label></h3>
</div>
<table class="table table-bordered" style=" margin-top: 1%;">
    <thead>
        <tr>
            <td>Type Name</td>
            <td style="width: 15%;">Action</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.TypeName
                </td>
                <td>
                    <a href="@Url.Action("Detail","Type")?ID=@item.Id" class="btn btn-info">Edit</a>
                    <a href="@Url.Action("Delete","Type")?ID=@item.Id" class="btn btn-danger" onclick="return confirm('Are you sure?');">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>


